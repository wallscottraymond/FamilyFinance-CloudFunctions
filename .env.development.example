# Backend - Cloud Functions Development Environment
# Copy this to .env.development and fill in your actual values

# Plaid Configuration (use Sandbox credentials)
PLAID_CLIENT_ID=your_dev_plaid_client_id
PLAID_SECRET=your_dev_sandbox_secret
PLAID_WEBHOOK_SECRET=your_dev_webhook_secret
PLAID_ENV=sandbox

# Encryption
TOKEN_ENCRYPTION_KEY=your_dev_encryption_key_64_characters_hex_string_here

# Environment
ENVIRONMENT=development
USE_EMULATOR=true
